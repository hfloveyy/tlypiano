.myvideo{width:100%;height:300rpx;margin:1px}.list{margin-top:40rpx;background-color:#fff;border-bottom:1px solid #eee}.list .item{height:400rpx;position:relative;box-sizing:border-box;padding:20rpx;border-bottom:1px solid #eee}.list .item .icon{width:80rpx;height:80rpx;position:absolute;margin-left:15rpx}.list .item .arrow_left{position:absolute;right:20rpx;width:30rpx;height:40rpx;margin-top:-32rpx}.list .item .title{padding:18rpx 0 0 100rpx;display:block;font-size:33rpx;color:#607B8B;font-family:"Times New Roman"}
